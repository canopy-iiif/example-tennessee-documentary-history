tems":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/6655/canvas/0/annopage-0/annotation-0","type":"Annotation","motivation":"commenting","body":{"type":"TextualBody","language":"en","format":"text/plain","value":"John Dickenson Esqr. Esquire Dear Sir/ We have this day recd. received a letter from Mr. Samuel Ferguson of New York , requesting us to recommend to him a Suitable Person in Nashville to collect a debt for him, we have recommended you to him, & have Enclosed this letter to him as an introductory letter to you. Mr. Ferguson is a friend of ours & we trust you will use your customary diligence in transacting any business he may entrust you with Mr Ferguson is a respectable Merchant living in the City of New York & we hope this introduction may be equally advantageous We remain respectfully your friends Neill & Harley Philada Philadelphia May 22"},"target":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/6655/canvas/0"}]}],"seeAlso":[{"id":"https://digital.lib.utk.edu/collections/islandora/object/tdh:6657/datastream/HOCR","type":"Dataset","label":{"none":["HOCR"]},"format":"text/vnd.hocr+html","profile":"http://kba.cloud/hocr-spec/1.2/"}]},{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/6655/canvas/1","type":"Canvas","label":{"none":["[Letter] 1815 May 22 [to] John Dickenson Esq[ui]r[e] / Neill &Harley : page 2"]},"width":3704,"height":2864,"thumbnail":[{"id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:6656~datastream~JP2/full/463,358/0/default.jpg","width":463,"height":358,"type":"Image","format":"image/jpeg","service":[{"@id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:6656~datastream~JP2","@type":"http://iiif.io/api/image/2/context.json","profile":"http://iiif.io/api/image/2/level2.json"}]}],"items":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/6655/canvas/1/page/tdh:6656","type":"AnnotationPage","it