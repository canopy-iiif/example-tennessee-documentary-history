":[{"id":"https://digital.lib.utk.edu/collections/islandora/object/tdh%3A5972/datastream/TN","width":200,"height":200,"type":"Image","format":"image/jpeg"}],"items":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0","type":"Canvas","label":{"none":["[Letter] 1850 June 5, Columbia [to] Mary House / George House : page 1"]},"width":3046,"height":3903,"thumbnail":[{"id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:5973~datastream~JP2/full/381,488/0/default.jpg","width":381,"height":488,"type":"Image","format":"image/jpeg","service":[{"@id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:5973~datastream~JP2","@type":"http://iiif.io/api/image/2/context.json","profile":"http://iiif.io/api/image/2/level2.json"}]}],"items":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0/page/tdh:5973","type":"AnnotationPage","items":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0/page/tdh:5973/656b6631c83dd","type":"Annotation","motivation":"painting","body":{"id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:5973~datastream~JP2/full/full/0/default.jpg","type":"Image","width":3046,"height":3903,"format":"image/jpeg","service":[{"@id":"https://digital.lib.utk.edu/iiif/2/collections~islandora~object~tdh:5973~datastream~JP2","@type":"http://iiif.io/api/image/2/context.json","profile":"http://iiif.io/api/image/2/level2.json"}]},"target":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0"}]}],"annotations":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0/annopage-0","type":"AnnotationPage","items":[{"id":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0/annopage-0/annotation-0","type":"Annotation","motivation":"commenting","body":{"type":"TextualBody","language":"en","format":"text/plain","value":"Columbia June 5/50 Dear Mary We are here, Toby, Tom and I— I last but not least shall leave here for \"Mount Pleasant\" by 11 oclock AM— Have done but little at collecting— limitted limited to Mc? 35¢ this morning— Everything looks dull here and in fact it is so where I have been— We made 35 miles yesterday pretty good— as I was detained 3 or 4 hours in Franklin — I hope you are all well and that you will continue so—I think of you every hour in the day but the fleas used me so badly last night, I forgot almost everything else— I have no time to write from here — but will write to you from Pulaski and also from Athens — Give my love to all— Kiss Sallie & Lou for me— with a bushel of love— I am Your own George"},"target":"https://digital.lib.utk.edu/notdereferenceable/assemble/manifest/tdh/5972/canvas/0"}]}],"seeAlso":[{"id":"https://digital.lib.utk.edu/collections/islandora/object/tdh:5973/datastream/HOCR","type":"Dataset","label":{"none":["HOCR"]},"format":"text/vnd.hocr+html","profile":"http://kba.cloud/hocr-spec/1.2/"}]}],"seeAlso":[{"id":"https://digital.lib.utk.edu/collections/islandora/object/tdh%3A5972/datastream/MODS","type":"Dataset","label":{"en":["Bibliographic Description in MODS"]},"format":"application/xml","profile":"http://www.loc.gov/standards/mods/v3/mods-3-5.xsd"}],"partOf":[{"id":"https://digital.lib.utk.edu/assemble/collection/collections/tdh","type":"Collection"}],"homepage":[{"id":"https://digital.lib.utk.edu/collections/islandora/object/tdh%3A5972","label":{"en":["[Letter] 1850 June 5, Columbia [to] Mary House / George House"]},"type":"Text","format":"text/html"}],"behavior":["paged"]},"related":["https://canopy-iiif.github.io/example-tennessee-documentary-history/api/facet/subject/tennessee-description-and-travel.json?sort=random","https://canopy-iiif.github.io/example-tennessee-documentary-history/api/facet/place/tennessee.json?sort=random","https://canopy-iiif.github.io/example-tennessee-documentary-history/api/facet/format/correspondence